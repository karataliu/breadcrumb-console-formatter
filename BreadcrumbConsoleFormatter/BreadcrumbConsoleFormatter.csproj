<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>Util.Extensions.Logging</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Util.Extensions.Logging.BreadcrumbConsoleFormatter</PackageId>
    <PackageProjectUrl>https://github.com/karataliu/breadcrumb-console-formatter</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>0.2.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="8.0.0" />
  </ItemGroup>

</Project>
